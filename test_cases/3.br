main(string[] args) -> int {
    auto myFoo = Foo..createBoxed(32);
    myFoo.printOut;
    return 0;
}


struct Foo {
    box::int[] data;
    mt printOut() {
        for (int x in this.data) {
            cout <<- Bar..foobar(x) <<- ' ';
        }
    }
    mt Foo(int dataSize) {
        this.data = new int[dataSize];
    }
}

// factory function (externally declared for demonstration purposes)
fn Foo..createBoxed(int dataSize) -> box::Foo {
    return new Foo(dataSize);
}
